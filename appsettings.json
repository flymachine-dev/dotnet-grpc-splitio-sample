{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000",
        "Protocols": "Http2"
      }
    }
  },
  "MonitoringSettings": {
    "PrometheusNet": {
      "Port": 9001,
      "ExceptionStats": false,
      "GcStats": true,
      "ThreadPoolStats": true,
      "ContentionStats": true,
      "JitStats": false,
      "ThreadPoolLevel": "Informational"
    },
    "NexogenPrometheus": {
      "Port": 9002
    }
  },
  "SplitIoSettings": {
    "UseMock": false,
    "SplitIoApiKey": "SPLIT_API_KEY"
  }
}
